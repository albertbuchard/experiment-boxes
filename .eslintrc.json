{
  "env": {
        "browser": true,
        "jest": true
    },
    "extends": ["airbnb"],
    "rules": {
        "semi": [2, "never"],
        "no-unexpected-multiline": 2,
        "no-console": 0,
        "no-plusplus": ["error", { "allowForLoopAfterthoughts": true }],
        "no-prototype-builtins": 0,
        "max-len": 0,
        "no-extend-native": 0,
        "no-param-reassign": 0,
        "no-underscore-dangle": 0,
        "no-nested-ternary": 0
    }
}
